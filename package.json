{
  "name": "@gigscom/embeds-js",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "type": "module",
  "main": "./dist/gigs-embeds.umd.cjs",
  "module": "./dist/gigs-embeds.js",
  "types": "./dist/gigs-embeds.d.ts",
  "exports": {
    ".": {
      "import": "./dist/gigs-embeds.js",
      "require": "./dist/gigs-embeds.umd.cjs"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "sideEffects": false,
  "devDependencies": {
    "@preact/preset-vite": "^2.8.1",
    "@storybook/addon-essentials": "^7.6.15",
    "@storybook/addon-interactions": "^7.6.15",
    "@storybook/addon-links": "^7.6.15",
    "@storybook/blocks": "^7.6.15",
    "@storybook/preact": "^7.6.15",
    "@storybook/preact-vite": "^7.6.15",
    "@storybook/test": "^7.6.15",
    "@tailwindcss/forms": "^0.5.7",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/preact": "^3.2.3",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.15",
    "fishery": "^2.2.2",
    "jsdom": "^24.0.0",
    "mitt": "^3.0.1",
    "msw": "^2.2.0",
    "postcss": "^8.4.35",
    "preact": "^10.19.4",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.6.15",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.7.2",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.2.2"
  },
  "files": [
    "lib",
    "dist",
    "docs",
    "!**/__tests__",
    "!**/__mocks__",
    "!**/__stories__"
  ],
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "dependencies": {
    "@modular-forms/preact": "^0.7.0",
    "@preact/signals": "^1.2.2"
  }
}
